<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         version="4.0">
    <servlet>
        <servlet-name>LandingServlet</servlet-name>
        <servlet-class>com.landing.controller.LandingServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>LandingServlet</servlet-name>
        <url-pattern>/home</url-pattern>
    </servlet-mapping>

    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
